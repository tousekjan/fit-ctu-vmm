{
  "name": "devstack",
  "version": "1.0.0",
  "description": "Dev stack",
  "main": "index.js",
  "author": "Cyril Urban",
  "license": "MIT",
  "scripts": {
    "ts:watch": "tsc -w --noEmit",
    "ts:check": "tsc --noEmit",
    "code:check": "npx run-p pretty:check ts:check test",
    "pretty:fix": "prettier -- --write \"src/**/*.ts*\"",
    "tslint:fix": "tslint --fix -c tslint.json -p .",
    "pretty:check": "prettier -- -l \"src/**/*.ts*\"",
    "build": "webpack --progress --config ./config/webpack.prod.js",
    "build:dev": "webpack-dev-server --config ./config/webpack.dev.js",
    "dev": "STAGE_CONFIG=localhost npm-run-all locale --parallel ts:watch build:dev",
    "dev:mock": "STAGE_CONFIG=mock npm-run-all locale --parallel ts:watch build:dev",
    "prod": "run-s code:check locale build",
    "test": "jest --env=jsdom",
    "locale": "i18next-scanner --config translate-config.js",
    "test:watch": "jest --maxWorkers=1 --watch --env=jsdom",
    "test:cover": "jest --coverage --env=jsdom"
  },
  "externals": {
    "conf.js": false
  },
  "resolutions": {
    "@types/react": "^16.9.0"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "antd": "^3.22.0",
